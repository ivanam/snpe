<br/>

<table id="tabla-altas_bajas_hora_notificadas" class="table table-striped table-bordered table-hover">
	<thead>
		<tr>
			<th>DNI</th>
            <th>Nombre</th>
            <th>Sit. Rev</th> 
            <th>Cargo</th>                           
            <th>Turno</th>
            <th>Curso</th>
            <th>Div</th>
            <th>Fecha Alta</th>
            <th>Notificaciones</th>
		</tr>
	</thead>
	<tbody>
		<% @cargos_notificados.each do |cargo| %>
			<tr>
				<td><%= cargo.persona.nro_documento %></td>
        		<td><%= cargo.persona.to_s %></td>
        		<td><%= cargo.situacion_revista %></td>
        		<td><%= Funcion.where(categoria: cargo.cargo).first.to_s %></td>
        		<td><%= cargo.turno %></td>
        		<td><%= cargo.anio %></td>
        		<td><%= cargo.division %></td>
        		<td><%= Util.fecha_a_es(cargo.fecha_alta) %></td>
        		<td><%= cargo.estado_actual %></td>
      		</tr>
    	<% end %>
	</tbody>
</table>
