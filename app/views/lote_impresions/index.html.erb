<div class="page-header">
  <h1>Listado de Impresiones</h1>
</div>

<div class="table-responsive">
  <table id="tabla-lote_impresion" class="table table-striped table-bordered table-hover" data-source = "<%= lote_impresions_path(format: :json) %>" >
    <thead>
      <tr>
            <th>Fecha impresion</th>
            <th>Observaciones</th>
      </tr>
    </thead>
    <tbody>
    </tbody>
  </table>
</div>


<script type="text/javascript">
    $(document).ready(function($) {
      $('#tabla-lote_impresion').dataTable({
        language : {"sUrl": "<%= asset_path('language.es.json') %>",},
        processing : true,
        serverSide : true,
        ajax : $('#tabla-lote_impresion').data('source'),
        pagingType: "simple",
      });
    });
</script>