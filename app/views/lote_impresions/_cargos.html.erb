<legend>Novedades de Cargos</legend>

<legend>Altas</legend>
<table class="table table-bordered table-striped">
  <thead>
    <tr>
      <th>Agente</th>         
      <th>Curso</th>
      <th>Situacion Revista</th>
      <th>Fecha Alta</th>
      <th>Fecha Baja</th>
    </tr>
  </thead>
  <tbody>
    <% @lote_impresion.alta_cargos.each do |c| %>
      <tr>
        <td><%= c.persona.nro_documento.to_s + ' - ' + c.persona.to_s %></td>
        <td><%= c.anio.to_s + ' / ' + c.division.to_s %></td>
        <td><%= c.situacion_revista %></td>
        <td><%= c.fecha_alta %></td>
        <td><%= c.fecha_baja %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<legend>Bajas</legend>
