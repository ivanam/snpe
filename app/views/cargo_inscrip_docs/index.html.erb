 <script type="text/javascript" src="https://cdn.datatables.net/v/dt/jszip-2.5.0/dt-1.10.18/b-1.5.4/b-flash-1.5.4/b-html5-1.5.4/datatables.min.js"></script>
<div class="page-header">
  <h1>Listado Detallado de Inscripciones</h1>
</div>

<div class="table-responsive">
  <table id="tabla-cargo_inscrip_doc" class="table table-striped table-bordered table-hover" data-source = "<%= cargo_inscrip_docs_path(format: :json) %>" >
    <thead>
      <tr>
            <th>Region</th>
            <th>Cargo</th>
            <th>Puntaje Total</th>
            <th>Rubro II</th>
            <th>Prioridad</th>            
            <th>DNI</th>
           <th class="text-center">Acciones</th>
      </tr>
    </thead>
    <tbody>
    </tbody>
  </table>
</div>


<script type="text/javascript">
    $(document).ready(function($) {
      $('#tabla-cargo_inscrip_doc').dataTable({
        language : {"sUrl": "<%= asset_path('language.es.json') %>",},
        processing : true,
        serverSide : true,
        ajax : $('#tabla-cargo_inscrip_doc').data('source'),
        pagingType: "simple",
        dom: 'Bflrtip',
        paginate: true,
        buttons: [
          {
            extend: 'excel',
            exportOptions: {
                columns: 'th:not(:last-child)', // exclude action column                    
            },
            text:'<span class="pull-right glyphicon glyphicon-export">Excel</span>',
            title: 'Confirmaciones (listado final) 2018'
          }
        ]
      });
    });
</script>