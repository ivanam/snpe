<div class="page-header">
  <h1>Listado Detallado de Inscripciones</h1>
</div>

<div class="table-responsive">
  <table id="tabla-cargo_inscrip_doc" class="table table-striped table-bordered table-hover" data-source = "<%= cargo_inscrip_docs_path(format: :json) %>" >
    <thead>
      <tr>
            <th>Region</th>
            <th>Cargo</th>
            <th>Puntaje Total</th>
            <th>Rubro II</th>
            <th>Prioridad</th>            
            <th>DNI</th>
           <th class="text-center">Acciones</th>
      </tr>
    </thead>
    <tbody>
    </tbody>
  </table>
</div>


<script type="text/javascript">
    $(document).ready(function($) {
      $('#tabla-cargo_inscrip_doc').dataTable({
        language : {"sUrl": "<%= asset_path('language.es.json') %>",},
        processing : true,
        serverSide : true,
        ajax : $('#tabla-cargo_inscrip_doc').data('source'),
        pagingType: "simple",
      });
    });
</script>