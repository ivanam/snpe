<%= form_for(@cargo_inscrip_docs, :html => { :class => "form-horizontal" }) do |f| %>
  <% if @cargo_inscrip_docs.errors.any? %>
    <div class="alert alert-danger alert-dismissable">
      <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
      <h4>No se pudo guardar el formulario, tiene los siguientes errores:</h4>

      <ul>
      <% @cargo_inscrip_docs.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="form-group">
    <%= f.label :inscripcion_id, :class => "col-sm-2 control-label" %>
    <div class="col-sm-10">
      <%= f.number_field :inscripcion_id, :class => "form-control" %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :persona_id, :class => "col-sm-2 control-label" %>
    <div class="col-sm-10">
      <%= f.number_field :persona_id, :class => "form-control" %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :cargosnds_id, :class => "col-sm-2 control-label" %>
    <div class="col-sm-10">
      <%= f.number_field :cargosnds_id, :class => "form-control" %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :cargo_id, :class => "col-sm-2 control-label" %>
    <div class="col-sm-10">
      <%= f.number_field :cargo_id, :class => "form-control" %>
    </div>
  </div>
    <div class="form-group">
    <%= f.label :nivel_id, :class => "col-sm-2 control-label" %>
    <div class="col-sm-10">
      <%= f.number_field :nivel_id, :class => "form-control" %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-offset-2 col-sm-10">

      <%= f.submit :class => "btn btn-primary", :value => "Guardar" %>
      <%= link_to cargo_inscrip_docs_path, :class => 'btn btn-default' do %>
        <span class="glyphicon glyphicon-list-alt"></span>
        Atr√°s
      <% end %>
    </div>
  </div>
<% end %>
