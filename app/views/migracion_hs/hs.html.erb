  <div class="page-header">
      <h1>Migrar</h1>
    </div>


   <form action="" method="GET" class="form-horizontal" role="form">
      <div class="form-group">
          <label for="inputEmail3" class="col-sm-2 control-label">Ingrese Escuela</label>
          <input class="text_field" id="esc"></input>&nbsp;&nbsp;&nbsp;&nbsp;<button type="button" id="boton" class="btn btn-primary btn-sm"><span class="glyphicon glyphicon-search"> Migrar hs</span></button>
      </div>
  </form>

  <form action="" method="GET" class="form-horizontal" role="form">
      <div class="form-group">
          <label for="inputEmail3" class="col-sm-2 control-label">Ingrese Escuela</label>
          <input class="text_field" id="esc2"></input>&nbsp;&nbsp;&nbsp;&nbsp;<button type="button" id="boton2" class="btn btn-primary btn-sm"><span class="glyphicon glyphicon-search"> Migrar cargos</span></button>
      </div>
  </form>



<script type="text/javascript">

  $( document ).ready(function() {
  
  $( "#boton" ).click( function() {

  var esc = parseInt($( "#esc" ).val());


    $.ajax({
      url: '/soft/snpe/migracion_hs/migrar_hs/'+esc,
      type: 'POST',
    })

});

  $( "#boton2" ).click( function() {

  var esc = parseInt($( "#esc2" ).val());


    $.ajax({
      url: '/soft/snpe/migracion_hs/migrar_cargos/'+esc,
      type: 'POST',
    })

});
  });
</script>