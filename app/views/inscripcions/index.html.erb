</div>


<script type="text/javascript">
    $(document).ready(function($) {
      $('#tabla-inscripcion').dataTable({
        language : {"sUrl": "<%= asset_path('language.es.json') %>",},
        processing : true,
        serverSide : true,
        ajax : $('#tabla-inscripcion').data('source'),
        pagingType: "simple",
