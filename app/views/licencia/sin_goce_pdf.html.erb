<div>
  <h3> Alta de licencia sin goce </h3>

  <table class="table table-bordered table-striped">
    <tr>
      <th>Agente </th>
      <th>Secuencia </th>
      <th>Establecimiento </th>
      <th>Descripcion </th>
      <th>Curso </th>
      <th>Articulo </th>
      <th>Inicio / Fin </th>
    </tr>
    <% @licencia_alta.each do |l|%>
      <tr>
        <td><%= l.agente %></td>
        <td><%= l.secuencia %></td>
        <td><%= l.establecimiento %></td>
        <td><%= l.descripcion %></td>
        <td><%= l.curso %></td>
        <td><%= l.articulo %></td>
        <td><%= l.fecha_desde %> / <%= l.fecha_hasta %> </td>
      </tr>
    <% end %>
  </table>

