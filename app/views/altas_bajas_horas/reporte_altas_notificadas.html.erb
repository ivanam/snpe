<legend class="legend-small">Altas - Horas Cátedra</legend>

<% @e = Establecimiento.find(session[:establecimiento])%>            
<div class="page-header">
  <h3>Escuela N° <%= @e.to_s %></h3>
</div>

<table id="tabla-altas_bajas_hora_notificadas" class="table table-striped table-bordered table-hover">
	<thead>
		<tr>
      <th>CUIT/CUIL</th>  
			<th style="width: 250px;">Apellido y nombre</th>
			<th>Sit. Rev</th>
			<th>Horas</th>              
			<th>Ciclo</th>
			<th>Curso</th>
			<th>Div</th>
			<th>Turno</th>
			<th>Codif.</th>
			<th>Fecha Alta</th>
			<th>Notificaciones</th>
		</tr>
	</thead>
	<tbody>
		<% @altas_notificadas.each do |alta| %>
			<tr>
        <td><%= alta.persona.cuil %></td>
        <td><%= alta.persona.to_s %></td>
        <td><%= alta.situacion_revista %></td>
        <td><%= alta.horas %></td>
        <td><%= alta.ciclo_carrera %></td>
        <td><%= alta.anio %></td>
        <td><%= alta.division %></td>
        <td><%= alta.turno %></td>
        <td><%= alta.codificacion.rjust(AltasBajasHora::LONGITUD_CODIGO,'0') %></td>
        <td><%= Util.fecha_a_es(alta.fecha_alta) %></td>
        <td><span class="label label-<%=alta.estados.last.color_estado%>"><%= alta.estado_actual %></td>
      </tr>
    <% end %>
	</tbody>
</table>


